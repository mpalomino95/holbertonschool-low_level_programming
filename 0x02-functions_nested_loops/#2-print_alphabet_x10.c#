#rint_alphabet_x10 - prints alphabet 10 times
*
gcc -Wall -pedantic -Werror -Wextra _putchar.c 2-main.c 2-print_alphabet_x10.c -o 2-alphabet_x10* Return: always 0;
*
*/
void print_alphabet_x10(void)
{
char i;
char h;

for (h = 0; h <= 9; h++)
{
for (i = 'a'; i <= 'z'; i++)
{
_putchar(i);
}
_putchar('\n');
}
}
